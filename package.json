{
  "name": "context-menu-plugin",
  "version": "1.0.0",
  "description": "右键菜单插件，支持Vue3和React双版本",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.cjs.js"
    },
    "./vue3": {
      "import": "./dist/vue3/index.esm.js",
      "require": "./dist/vue3/index.cjs.js"
    },
    "./react": {
      "import": "./dist/react/index.esm.js",
      "require": "./dist/react/index.cjs.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "jest",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "context-menu",
    "right-click",
    "vue3",
    "react",
    "plugin"
  ],
  "author": "codefish <passerby365@163.com>",
  "license": "MIT",
  "peerDependencies": {
    "react": "^16.8.0 || ^17.0.0 || ^18.0.0",
    "react-dom": "^16.8.0 || ^17.0.0 || ^18.0.0",
    "vue": "^3.2.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/XcodeFish/context-menu-plugin"
  },
  "bugs": {
    "url": "https://github.com/XcodeFish/context-menu-plugin/issues"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.4.0",
    "@fortawesome/free-solid-svg-icons": "^6.4.0",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@fortawesome/vue-fontawesome": "^3.0.3",
    "@testing-library/jest-dom": "^5.0.0",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.5.14",
    "@vitejs/plugin-react": "^3.0.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/compiler-sfc": "^3.2.0",
    "animate.css": "^4.1.1",
    "autoprefixer": "^10.0.0",
    "jest": "^29.0.0",
    "postcss": "^8.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "rollup": "^3.0.0",
    "sass": "^1.58.0",
    "vite": "^4.0.0",
    "vue": "^3.2.0"
  }
}
